{
  "name": "snake",
  "version": "1.0.0",
  "description": "The classic arcade game in which a player controls a snake, guiding it towards food and away from walls and itself.",
  "author": "Rishad Bharucha",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "webpack --config config/webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --open --config config/webpack-dev.config.js --watch --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rishadfb/snake"
  },
  "bugs": {
    "url": "https://github.com/rishadfb/snake/issues"
  },
  "homepage": "https://github.com/rishadfb/snake/README.md",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0"
  },
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.1",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.17.0",
    "node-sass": "^4.7.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  }
}
